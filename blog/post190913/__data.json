！🚶🏻‍♂🚶🏻‍♀\n\n食物中，油脂較高的魚類（例如三文魚、沙甸魚、吞拿魚）、蛋黃、肝臟等也含較豐富維他命D呢！😉","post190913","2019-09-13T00:00:00.000Z",{"data":4},{"data":10},[11],{"id":12,"attributes":13},1,{"slug":14,"name":15,"description":4,"createdAt":16,"updatedAt":16},"nutrition","營養隨筆","2023-02-12T02:39:02.691Z",{"data":18},[19,24],{"id":20,"attributes":21},133,{"name":22,"createdAt":23,"updatedAt":23},"維他命D","2023-02-12T03:32:01.328Z",{"id":25,"attributes":26},134,{"name":27,"createdAt":28,"updatedAt":28},"血清素","2023-02-12T03:32:01.338Z",{"data":4},{"data":31},[]],"uses":{"dependencies":["http://127.0.0.1:1337/api/posts?populate[author]=true&populate[categories]=true&populate[tags]=true&populate[image]=true&populate[related_posts]=true&filters[slug][$eq]=post190913"],"params":["slug"]}}]}